{
  "name": "golfiq-store-tooling",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "typecheck:web": "npm --prefix web run typecheck",
    "typecheck:mobile": "npm --prefix apps/mobile run typecheck",
    "build:web": "npm --prefix web run build",
    "test:mobile:vitest": "vitest run --workspace vitest.workspace.ts --project mobile",
    "test:unit": "npm --prefix web run test:unit",
    "test:web": "npm --prefix web run test:unit",
    "test:web:ci": "npm --prefix web run test",
    "ci:web": "npm run typecheck:web && npm run build:web && npm run test:web:ci",
    "ci:mobile": "npm run test:mobile:vitest && npm run typecheck:mobile",
    "ci:local": "npm run ci:web && npm run ci:mobile",
    "demo:sg": "node --loader ts-node/esm web/scripts/seedS16Demo.ts",
    "store:screens": "tsx --tsconfig tsconfig.tools.json tools/store_screenshots.ts",
    "store:privacy": "python3 tools/privacy_labels.py",
    "store:zip": "bash scripts/make_store_zip.sh",
    "store:attach": "tsx --tsconfig tsconfig.tools.json tools/attach_to_release.ts",
    "audit:inventory": "if [ -f tools/inventory_audit.py ]; then python tools/inventory_audit.py; else echo 'tools/inventory_audit.py missing; skipping audit'; fi"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.1",
    "@supabase/supabase-js": "^2.45.0",
    "puppeteer": "^22.15.0"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "jsdom": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.4"
  }
}
