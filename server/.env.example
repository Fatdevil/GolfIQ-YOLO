# --- Staging defaults ---
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_TIMEOUT=3
LLM_ENABLED=true
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini
TTS_ENABLED=false
TTS_PROVIDER=openai
TTS_VOICE=alloy
COACH_PROVIDER=openai
COACH_FEATURE=false

# Cloud sync (Supabase)
# Accepts: true|1|yes|on (case-insensitive). Any other value => off.
CLOUD_SYNC_ENABLED=false
CLOUD_SNAPSHOT_TTL_DAYS=30

# YOLO runtime
YOLO_INFERENCE=false
YOLO_MODEL_PATH=

# Security / CORS
API_KEY=
CORS_ORIGINS=http://localhost:19006,http://localhost:5173

# Data retention
# Optional: retention sweeping (comma-separated dirs)
RETENTION_DIRS=
# Minutes before a file is considered old
RETENTION_MINUTES=15

# Run upload storage
STORAGE_BACKEND=s3
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
S3_BUCKET=golfiq-runs
S3_ACCESS_KEY=changeme
S3_SECRET_KEY=changeme
S3_FORCE_PATH_STYLE=1
RUNS_TTL_DAYS=30

# ===== CaddieCore v1 (dispersion engine) =====
CADDIE_KSIGMA_MAIN=1.0
CADDIE_KSIGMA_CONSERVATIVE=1.5
CADDIE_HAZARD_BUFFER_M=5
CADDIE_WIND_HEAD_COEF=1.5
CADDIE_WIND_TAIL_COEF=-1.2
CADDIE_WIND_CROSS_LATERAL_M_PER_MS=0.5
CADDIE_ELEVATION_PER_M=0.8
CADDIE_LIE_ROUGH_EXTRA_M=5
CADDIE_LIE_ROUGH_EXTRA_SIGMA=1
